{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://opendi.org/json-schema/v0-draft-0325/UUID.json",
    "title": "UUID",
    "$comment": "Ensures UUID validation is consistent.",
    "type": "string",
    "format": "uuid",
    "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$"
}